{
  "name": "guild-telegram-runner",
  "version": "0.0.1",
  "description": "A Telegram bot for the Guild project",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --exit-child src/Main.ts",
    "build": "tsc",
    "prod": "node build/Main.js"
  },
  "dependencies": {
    "@types/redis": "^2.8.31",
    "axios": "^0.21.1",
    "dayjs": "^1.11.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.11.1",
    "telegraf": "^4.7.0",
    "typegram": "^3.8.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.2.0",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix --cache --cache-location 'node_modules/.cache/.eslintcache'",
    "*.{js,ts,md}": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agoraxyz/telegram-runner.git"
  },
  "keywords": [
    "agora",
    "space",
    "guild",
    "telegram",
    "bot",
    "crypto",
    "token",
    "tokenized",
    "community"
  ],
  "author": "ZGEN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agoraxyz/telegram-runner/issues"
  },
  "homepage": "https://github.com/agoraxyz/telegram-runner#readme"
}
